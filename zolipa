local SETTINGS = {
    -- AIM SYSTEM
    AimEnabled = true,
    AimKey = Enum.KeyCode.LeftShift,
    FOV = 90,                  -- Увеличенный угол захвата
    AimDelay = 0.05,           -- Уменьшенная задержка
    Smoothness = 0.15,         -- Более плавное движение
    TeamCheck = true,
    VisibilityCheck = true,
    AutoFire = false,          -- Новый функционал
    
    -- ESP SYSTEM
    ESPEnabled = true,
    BoxESP = true,
    NameESP = true,
    HealthBar = true,
    DistanceESP = true,
    VisibleColor = Color3.fromRGB(255, 50, 50),
    HiddenColor = Color3.fromRGB(50, 50, 255),
    ESPTransparency = 0.4,     -- Лучшая видимость
    
    -- UI
    MenuKey = Enum.KeyCode.RightShift,
    UIScale = 1               -- Масштабирование интерфейса
}

-- ███╗   ███╗███████╗███╗   ██╗██╗   ██╗
